[alias]
    co = checkout
    lg = log --all --graph --decorate --oneline --abbrev-commit
    cm = commit
    ac = !git add -A && git commit
    st = status -sb
    tags = tag -l
    branches = branch -a
    rmt = remote -v
    pl = !git pull --commit && git push
